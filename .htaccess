RewriteEngine On
RewriteBase /

# Protect the htaccess file
<Files .htaccess>
Order Allow,Deny
Deny from all
</Files>

# Protect keys.json
<Files ./keys.json>
Order Allow,Deny
Deny from all
</Files>

LoadModule php_module "c:\php\php8apache2_4.dll"
<FilesMatch \.php$>
    SetHandler application/x-httpd-php
</FilesMatch>
